
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class='login-form'>
  <div class='login-header'>
    <div class='back'>
      <%= link_to '<', groups_path%>
      <span>â€”</span>
    </div>
    <h4>LOGIN</h4>
    <div class="actions">
      <%= f.submit "Log in" %>
    </div>
  </div>
  
  <div class='login-fields'>
    <div class="field">
      <%= f.email_field :email, autofocus: true, placeholder: 'Email', autocomplete: "email" %>
    </div>
    <div class="field">
      <%= f.password_field :password, placeholder: 'Password', autocomplete: "current-password" %>
    </div>
  </div>
  <%= link_to "Forgot your password?", new_password_path(resource_name), class: 'forgot' %><br />
</div>
<% end %>




